<div class="table-remove my-5">
  <div class="container">
    <div class="card">
      <div class="card-body">
        <img src="/images/logo2.png" alt="" height="35" class="my-3" />
        <table class="table table-striped">
          <thead>
            <tr>
              <th scope="col">No</th>
              <th scope="col" style="text-align: center;">Via</th>
              <th scope="col" style="text-align: center;">Username</th>
              <th scope="col" style="text-align: center;">Email</th>
              <th scope="col" style="text-align: center;">Contact</th>
              <th scope="col" style="text-align: center;">Created At</th>
              <th scope="col" style="text-align: end;">delete</th>
            </tr>
          </thead>
          <tbody>
            {{#each data}}
            <tr>
              <th scope="row">{{@index}}</th>
              <td style="text-align: center; font-weight:bolder">{{this.via}}</td>
              <td style="text-align: center; font-weight:bolder">{{this.username}}</td>
              <td style="text-align: center; font-weight:bolder">{{this.email}}</td>
              <td style="text-align: center; font-weight:bolder">{{this.via}}</td>
              <td style="text-align: center; font-weight:bolder">{{this.createdAt}}</td>
              <td style="text-align: end;">
                <button
                  onclick="showWarning('{{this._id}}')"
                  type="button"  
                  style="border: none;"
                ><i class='bx bxs-trash'></i></button>
              </td>
            </tr>
            {{/each}}
          </tbody>
        </table>
        <div class="input text-end mt-4">
          <button
            onclick="document.getElementById('add-admin').style.display='block'"
          >Add Admin</button>
        </div>
      </div>
    </div>
  </div>
</div>